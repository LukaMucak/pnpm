{
  "name": "@pnpm/workspace.update-injected-packages",
  "version": "1000.0.0-0",
  "description": "Update all injected replica of a workspace package",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "!*.map"
  ],
  "engines": {
    "node": ">=18.12"
  },
  "scripts": {
    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\"",
    "_test": "jest",
    "test": "pnpm run compile && pnpm run _test",
    "prepublishOnly": "pnpm run compile",
    "compile": "tsc --build && pnpm run lint --fix"
  },
  "repository": "https://github.com/pnpm/pnpm/blob/main/workspace/update-injected-packages",
  "keywords": [
    "pnpm10",
    "pnpm"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pnpm/pnpm/issues"
  },
  "homepage": "https://github.com/pnpm/pnpm/blob/main/workspace/update-injected-packages#readme",
  "funding": "https://opencollective.com/pnpm",
  "dependencies": {
    "@pnpm/directory-fetcher": "workspace:*",
    "@pnpm/dependency-path": "workspace:*",
    "@pnpm/error": "workspace:*",
    "@pnpm/fs.indexed-pkg-importer": "workspace:*",
    "@pnpm/lockfile.fs": "workspace:*",
    "@pnpm/logger": "workspace:*",
    "@pnpm/types": "workspace:*"
  },
  "devDependencies": {
    "@pnpm/workspace.update-injected-packages": "workspace:*"
  },
  "exports": {
    ".": "./lib/index.js"
  },
  "jest": {
    "preset": "@pnpm/jest-config"
  }
}
